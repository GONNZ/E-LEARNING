@model IEnumerable<E_LEARNING.Models.CursoProfe>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>Curso</th>
        <th>Profesor</th>
        <td>Opciones</td>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>@item.Curso.NombreCurso</td>
        <td>@item.Profe.nombre</td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdCursoProfe }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdCursoProfe }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdCursoProfe })
        </td>
    </tr>
}

</table>
